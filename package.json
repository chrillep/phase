{
  "name": "@phased/core",
  "private": true,
  "workspaces": [
    "packages/@phased/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@sucrase/jest-plugin": "^2.0.0",
    "brotli-size": "^4.0.0",
    "chalk": "^4.0.0",
    "esm": "^3.2.25",
    "execa": "^4.0.0",
    "gzip-size": "^5.1.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lerna": "^3.20.2",
    "minimist": "^1.2.0",
    "prettier": "^2.0.5",
    "pretty-bytes": "^5.3.0",
    "rollup": "^2.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "tslib": "^2.0.0",
