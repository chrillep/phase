{
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Phased\\": "packages/Phased"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phased\\Tests\\": "tests/"
        }
    },
    "replace": {
        "phased/routing": "self.version",
        "phased/state": "self.version"
    }
}
