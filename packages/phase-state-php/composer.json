{
    "name": "phased/state",
    "description": "Magic Data Loading. Laravel <-> Vuex Syncing",
    "type": "library",
    "license": "ISC",
    "authors": [
        {
            "name": "Reed Jones",
            "email": "reedjones@reedjones.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phased\\State\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phased\\State\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phased\\State\\PhasedStateServiceProvider"
            ],
            "aliases": {
                "Vuex": "Phased\\State\\Facades\\Vuex"
            }
        }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.4"
    }
}
