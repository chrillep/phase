{
    "name": "phased/state",
    "description": "Magic Data Loading. Laravel <-> Vuex Syncing",
    "type": "library",
    "license": "ISC",
    "homepage": "https://phased.dev",
    "support": {
        "issues": "https://github.com/reed-jones/phase/issues",
        "source": "https://github.com/reed-jones/phase"
    },
    "authors": [
        {
            "name": "Reed Jones",
            "email": "reedjones@reedjones.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phased\\State\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Phased\\State\\PhasedStateServiceProvider"
            ],
            "aliases": {
                "Vuex": "Phased\\State\\Facades\\Vuex"
            }
        },
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "require": {
        "php": "^7.2"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
