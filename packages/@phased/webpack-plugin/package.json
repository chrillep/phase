{
    "name": "@phased/webpack-plugin",
    "version": "0.0.1",
    "description": "Laravel Mix Plugin for Phased",
    "author": "Reed Jones <reedjones@reedjones.com>",
    "homepage": "",
    "license": "ISC",
    "main": "dist/phased-webpack-plugin.cjs.js",
    "module": "dist/phase-webpack-plugin.esm.js",
    "browser": "dist/phase-webpack-plugin.umd.js",
    "directories": {
      "lib": "lib",
      "test": "__tests__"
    },
    "files": [
      "dist"
    ],
    "scripts": {
      "compile": "rollup -c",
      "dev": "rollup -c -w",
      "build": "yarn types && NODE_ENV=production yarn compile",
      "test": "jest",
      "pretest": "yarn compile",
      "types": "tsc",
      "check": "tsc --noEmit --emitDeclarationOnly false"
    },
    "devDependencies": {
      "@rollup/plugin-alias": "^3.0.0",
      "@rollup/plugin-commonjs": "^11.0.1",
      "@rollup/plugin-node-resolve": "^7.0.0",
      "@rollup/plugin-sucrase": "^3.0.0",
      "@sucrase/jest-plugin": "^2.0.0",
      "jest": "^24.9.0",
      "laravel-mix": "^5.0.1",
      "rollup": "^1.29.0",
      "rollup-plugin-terser": "^5.2.0",
      "tslib": "^1.10.0",
      "typescript": "^3.7.5"
    },
    "publishConfig": {
      "registry": "https://registry.yarnpkg.com"
    },
    "dependencies": {
      "@phased/routing": "^0.0.1",
      "@phased/types": "^0.0.1",
      "@phased/webpack-plugin": "^0.0.1"
    },
    "peerDependencies": {
        "webpack": "^4.41.5"
    }
  }
