{
  "name": "vuexcellent",
  "version": "0.0.0",
  "description": "Vuex hydration and auto mutator",
  "author": "Reed Jones <reedjones@reedjones.com>",
  "homepage": "",
  "license": "ISC",
  "main": "dist/vuexcellent.cjs.js",
  "module": "dist/vuexcellent.esm.js",
  "browser": "dist/vuexcellent.umd.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "scripts": {
    "compile": "rollup -c",
    "dev": "rollup -c -w",
    "build": "yarn types && NODE_ENV=production yarn compile",
    "test": "jest",
    "pretest": "yarn compile",
    "types": "tsc",
    "check": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/axios": "^0.14.0",
    "jest": "^24.9.0",
    "rollup": "^1.29.0",
    "rollup-plugin-terser": "^5.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "dependencies": {
    "axios": "^0.19.1"
  }
}
